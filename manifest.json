{
    "manifest_version": 3,
    "name": "Leetcode Problem Counter",
    "version": "1.0",
    "description": "Counts number of leetcode problems in an SDE sheet",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
  ],
  
    "action": {
      "default_popup": "popup.html",
      "default_icon":"images/LeetcodeExtension.png"  
    },
    "icons": {
      "16": "images/LeetcodeExtension.png",
      "48": "images/LeetcodeExtension.png",
      "128": "images/LeetcodeExtension.png"
    }
  }